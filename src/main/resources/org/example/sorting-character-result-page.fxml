<AnchorPane prefHeight="600.0" prefWidth="940.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SortingCharacterResultPageController">
   <children>
      <ImageView pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Image/AOT%20characters%20result.jpg" />
         </image>
      </ImageView>
      <Pane blendMode="OVERLAY" layoutX="127.0" layoutY="134.0" prefHeight="43.0" prefWidth="686.0" style="-fx-background-color: black;">
         <children>
            <Label layoutX="41.0" layoutY="13.0" text="Ranking" textFill="WHITE">
               <font>
                  <Font name="Century Schoolbook" size="18.0" />
               </font></Label>
            <Label layoutX="315.0" layoutY="13.0" text="Name" textFill="WHITE">
               <font>
                  <Font name="Century Schoolbook" size="18.0" />
               </font></Label>
            <Label layoutX="594.0" layoutY="13.0" text="Value" textFill="WHITE">
               <font>
                  <Font name="Century Schoolbook" size="18.0" />
               </font></Label>
         </children>
      </Pane>
      <ScrollPane blendMode="HARD_LIGHT" layoutX="127.0" layoutY="178.0" prefHeight="377.0" prefWidth="686.0" style="-fx-background-color: ANTIQUEWHITE;">
         <content>
            <VBox fx:id="vBox" prefHeight="0.0" prefWidth="0.0" />
         </content>
         <effect>
            <Blend />
         </effect>
      </ScrollPane>
      <Label fx:id="RankingLabel" blendMode="EXCLUSION" layoutX="255.0" layoutY="59.0" style="-fx-background-color: ALICEBLUE; -fx-background-insets: 4;" text="Attribute's Ranking">
         <font>
            <Font name="Copperplate Gothic Bold" size="38.0" />
         </font>
         <effect>
            <Reflection />
         </effect>
      </Label>
      <Button layoutX="828.0" layoutY="40.0" mnemonicParsing="false" onAction="#ExitButtonPressed" style="-fx-background-color: LIGHTCORAL; -fx-background-radius: 100;" text="X">
         <font>
            <Font name="Copperplate Gothic Bold" size="20.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
