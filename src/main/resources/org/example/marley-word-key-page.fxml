<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="250.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.MarleyWordKeyController">
   <children>
      <Label layoutX="95.0" layoutY="54.0" prefHeight="35.0" prefWidth="312.0" text="Please enter the key to decrypt the message!">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="inputKey" layoutX="134.0" layoutY="125.0" promptText="Enter the key here" />
      <Button layoutX="292.0" layoutY="125.0" mnemonicParsing="false" onMouseClicked="#GoButtonPressed" text="Go" />
      <Label fx:id="errorLabel" layoutX="134.0" layoutY="165.0" prefHeight="25.0" prefWidth="181.0" textFill="RED" />
   </children>
</AnchorPane>
