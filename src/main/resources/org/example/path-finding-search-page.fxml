            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Imprint MT Shadow" size="20.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <Label fx:id="errorLabel" layoutX="297.0" layoutY="261.0" prefHeight="18.0" prefWidth="382.0" textFill="RED">
         <font>
            <Font name="Consolas" size="13.0" />
         </font></Label>
      <Text fill="#fff600" layoutX="298.0" layoutY="340.0" strokeType="OUTSIDE" strokeWidth="0.0" text="( Starting point from 0 to 15 )" wrappingWidth="343.73675537109375">
