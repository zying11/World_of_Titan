      <ImageView pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Image/AOT%20path%20finding%20action.jpg" />
         </image>
      </ImageView>
      <Text fill="#be1a1a" layoutX="45.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="To notice all titans' position..." textAlignment="CENTER" wrappingWidth="231.3942108154297">
         <font>
            <Font name="Juice ITC" size="54.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Text>
      <TextField fx:id="startingPoint" layoutX="297.0" layoutY="279.0" prefHeight="41.0" prefWidth="293.0" promptText="Enter starting point">
         <font>
            <Font name="Lucida Sans Typewriter Regular" size="14.0" />
         </font></TextField>
      <Button layoutX="591.0" layoutY="279.0" mnemonicParsing="false" onMouseClicked="#GoSearchPathButtonPressed" prefHeight="41.0" prefWidth="52.0" style="-fx-background-color: lightgreen;" text="GO">
