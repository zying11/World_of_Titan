            <InnerShadow />
         </effect>
      </Label>
      <Label blendMode="HARD_LIGHT" layoutX="108.0" layoutY="91.0" style="-fx-background-color: ivory;" text="Value:">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <InnerShadow />
