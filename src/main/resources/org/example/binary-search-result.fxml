      <ImageView fitHeight="300.0" fitWidth="601.0" layoutY="-24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Image/AOT%20binary%20search%20result.jpg" />
         </image>
      </ImageView>
      <Label fx:id="outputNames" alignment="CENTER" contentDisplay="CENTER" layoutX="35.0" layoutY="170.0" prefHeight="19.0" prefWidth="530.0" textFill="#fffb00">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <DropShadow />
         </effect></Label>
      <Label blendMode="HARD_LIGHT" layoutX="199.0" layoutY="56.0" style="-fx-background-color: IVORY;" text="Ability:">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Label>
      <Label blendMode="HARD_LIGHT" layoutX="354.0" layoutY="56.0" style="-fx-background-color: ivory;" text="Value:">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Label>
      <Label blendMode="HARD_LIGHT" layoutX="245.0" layoutY="138.0" style="-fx-background-color: IVORY;" text="Soldier's Name:">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Label>
      <Label fx:id="outputAbility" alignment="CENTER" contentDisplay="CENTER" layoutX="170.0" layoutY="85.0" prefHeight="19.0" prefWidth="110.0" textFill="#fffb00">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <DropShadow />
         </effect></Label>
      <Label fx:id="outputAbilityValue" alignment="CENTER" contentDisplay="CENTER" layoutX="321.0" layoutY="85.0" prefHeight="19.0" prefWidth="110.0" textFill="#fffb00">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <DropShadow />
         </effect></Label>
      <Button layoutX="277.0" layoutY="215.0" mnemonicParsing="false" onAction="#OKButtonPressed" prefHeight="17.0" prefWidth="45.0" style="-fx-background-color: MISTYROSE;" text="OK">
         <font>
            <Font name="Constantia Bold" size="15.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
