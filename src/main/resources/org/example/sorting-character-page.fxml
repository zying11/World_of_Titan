<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="940.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SortingCharacterPageController">
   <children>
      <Label layoutX="341.0" layoutY="132.0" text="Character Sorting">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <Button layoutX="845.0" layoutY="541.0" mnemonicParsing="false" onMouseClicked="#HomeButtonPressed" prefHeight="40.0" prefWidth="75.0" text="Home">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button layoutX="59.0" layoutY="534.0" mnemonicParsing="false" onAction="#BackToCharacterButtonPressed" prefHeight="40.0" prefWidth="75.0" text="Back">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Label layoutX="292.0" layoutY="241.0" text="Search for your soldier based on their attributes ">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button layoutX="655.0" layoutY="320.0" mnemonicParsing="false" onAction="#SearchAttributeButtonPressed" prefHeight="40.0" text="search">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextField fx:id="inputAttribute" layoutX="296.0" layoutY="320.0" prefHeight="40.0" prefWidth="358.0" promptText="Enter attribution" />
      <Label fx:id="errorLabel" layoutX="297.0" layoutY="371.0" prefHeight="28.0" prefWidth="358.0" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
