            <Font name="Niagara Engraved" size="55.0" />
         </font>
      </Text>
      <TextField fx:id="locationOfTitan" layoutX="278.0" layoutY="313.0" prefHeight="41.0" prefWidth="306.0" promptText="Enter location of titan (1-15)">
