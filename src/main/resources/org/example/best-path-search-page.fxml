<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="600.0" prefWidth="940.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.BestPathSearchPageController">
   <children>
      <Text layoutX="248.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Find out best path to kill titan">
         <font>
            <Font size="34.0" />
         </font>
      </Text>
      <TextField fx:id="locationOfTitan" layoutX="278.0" layoutY="300.0" prefHeight="54.0" prefWidth="306.0" promptText="Enter location of titan (1-15)" />
      <Button layoutX="584.0" layoutY="300.0" mnemonicParsing="false" onMouseClicked="#GoFindBestPathButtonPressed" prefHeight="54.0" prefWidth="64.0" text="Go">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Text layoutX="409.0" layoutY="201.0" strokeType="OUTSIDE" strokeWidth="0.0" text="( you are at location 0 )" />
      <Label fx:id="errorLabel" layoutX="278.0" layoutY="356.0" prefHeight="18.0" prefWidth="372.0" textFill="RED" />
      <Button layoutX="55.0" layoutY="528.0" mnemonicParsing="false" onMouseClicked="#BackButtonPressed" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
