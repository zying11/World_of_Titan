            <Font name="Lucida Sans Typewriter Regular" size="13.0" />
         </font></TextField>
      <Button layoutX="584.0" layoutY="313.0" mnemonicParsing="false" onMouseClicked="#GoFindBestPathButtonPressed" prefHeight="41.0" prefWidth="64.0" style="-fx-background-color: GREENYELLOW;" text="GO">
