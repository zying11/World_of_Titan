<AnchorPane prefHeight="600.0" prefWidth="940.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.WeakestWallSearchPageController">
   <children>
      <Label layoutX="237.0" layoutY="83.0" prefHeight="114.0" prefWidth="466.0" text="Find weakest part of the wall">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="numberOfLayers" layoutX="275.0" layoutY="252.0" prefHeight="26.0" prefWidth="286.0" promptText="Enter number of layers" />
