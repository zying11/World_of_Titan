      <Button layoutX="603.0" layoutY="452.0" mnemonicParsing="false" onMouseClicked="#GoFindWeakestPart" text="Go" />
      <TextArea fx:id="numberOfEdges" layoutX="275.0" layoutY="287.0" prefHeight="191.0" prefWidth="286.0" promptText="Enter brick edges of each layer " />
      <Label fx:id="errorLabel" layoutX="275.0" layoutY="513.0" prefHeight="18.0" prefWidth="506.0" textFill="RED" />
      <Text layoutX="275.0" layoutY="500.0" strokeType="OUTSIDE" strokeWidth="0.0" text="( Edges value start from 1 to 10 )" />
   </children>
</AnchorPane>
